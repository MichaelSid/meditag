<!DOCTYPE html>
<html>
<head>
  <title>Meditag</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>


<%= render 'layouts/header' %>

    <div id="wrapper">
      <div id="page-wrapper">
        <div class="container">
          
          <% if notice %>
          <div class="col-md-12">
            <div class="alert alert-info"><%= notice %></div>
          </div>
          <% end %>

          <% if alert %>
          <div class="col-md-12">
            <div class="alert alert-danger"><%= alert %></div>
          </div>
          <% end %>
          
          <%= yield %>

        </div>

    </div><!-- /#page-wrapper -->
  </div><!-- /#wrapper -->

</body>
</html>
